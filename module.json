{
  "id": "actor-widgets",
  "title": "Actor Widgets",
  "description": "Opens an API to an app for rendering actor data in the browser.",
  "version": "#{VERSION}#",
  "manifestPlusVersion": "1.2.0",
  "compatibility": {
    "minimum": 11,
    "verified": 11,
    "maximum": 11
  },
  "authors": [
    {
      "name": "Chuey",
      "url": "https://github.com/barrientosjesus",
      "discord": "chuey"
    }
  ],
  "relationships": {
    "systems": [
      {
        "id": "dnd5e",
        "type": "system",
        "manifest": "https://raw.githubusercontent.com/foundryvtt/dnd5e/master/system.json",
        "compatibility": {
          "minimum": "2.3.0",
          "verified": "2.3.1"
        }
      }
    ],
    "requires": []
  },
  "esmodules": [
    "scripts/actorviewer.js"
  ],
  "scripts": [
    "scripts/dnd5e.js",
    "scripts/foundryFilePicker.js"
  ],
  "styles": [
    "styles/externalactor.css"
  ],
  "languages": [
    {
      "lang": "en",
      "name": "English",
      "path": "language/en.json"
    }
  ],
  "url": "#{URL}#",
  "socket": true,
  "manifest": "#{MANIFEST}#",
  "download": "#{DOWNLOAD}#",
  "license": "LICENSE",
  "readme": "README.md",
  "media": [
    {
      "type": "icon",
      "url": "https://i.imgur.com/dqxOv6y.png"
    }
  ]
}